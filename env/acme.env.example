# Cloudflare DNS-01 challenge token (must have Zone:DNS:Edit permission)
CF_Token=your-cloudflare-api-token-here

# Optional (only needed if you're using a scoped token that requires account/zone IDs)
# CF_Account_ID=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
# CF_Zone_ID=yyyyyyyyyyyyyyyyyyyyyyyyyyyyyyyy

# Safety margin for DNS propagation (in seconds)
# 180-300 is usually safe with Cloudflare
ACME_DNSSLEEP=180

# Optional: increase verbosity during first runs / debugging
ACME_DEBUG=1